#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/boot/Public/www/htmx-notes/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/nopt@1.0.10/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/node_modules:/home/boot/Public/www/htmx-notes/node_modules:/home/boot/Public/www/node_modules:/home/boot/Public/node_modules:/home/boot/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
